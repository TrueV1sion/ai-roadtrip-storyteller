{
  "name": "roadtrip-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov --coverageReporters=html --coverageReporters=json-summary",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint .",
    "start:mvp": "EXPO_PUBLIC_MVP_MODE=true expo start",
    "android:mvp": "EXPO_PUBLIC_MVP_MODE=true expo start --android",
    "ios:mvp": "EXPO_PUBLIC_MVP_MODE=true expo start --ios",
    "build:ios:mvp": "EXPO_PUBLIC_MVP_MODE=true expo build:ios --release-channel mvp",
    "build:android:mvp": "EXPO_PUBLIC_MVP_MODE=true expo build:android --release-channel mvp"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^7.0.18",
    "@react-navigation/stack": "^7.2.2",
    "@reduxjs/toolkit": "^2.6.1",
    "axios": "^1.6.7",
    "expo": "~50.0.6",
    "expo-av": "^15.0.2",
    "expo-blur": "^14.1.5",
    "expo-constants": "~15.4.5",
    "expo-haptics": "^14.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-location": "~16.5.0",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-maps": "1.10.0",
    "react-native-reanimated": "^3.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.27.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.45",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.7",
    "prettier": "^3.2.5",
    "typescript": "^5.3.0"
  },
  "private": true
}
