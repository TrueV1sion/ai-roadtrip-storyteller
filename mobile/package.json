{
  "name": "roadtrip-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov --coverageReporters=html --coverageReporters=json-summary",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint .",
    "start:mvp": "EXPO_PUBLIC_MVP_MODE=true expo start",
    "android:mvp": "EXPO_PUBLIC_MVP_MODE=true expo start --android",
    "ios:mvp": "EXPO_PUBLIC_MVP_MODE=true expo start --ios",
    "build:ios:mvp": "EXPO_PUBLIC_MVP_MODE=true expo build:ios --release-channel mvp",
    "build:android:mvp": "EXPO_PUBLIC_MVP_MODE=true expo build:android --release-channel mvp",
    "build:production": "./scripts/build-production.sh",
    "build:web": "expo export --platform web",
    "build:obfuscated": "NODE_ENV=production expo build --config metro.config.obfuscation.js",
    "bundle:obfuscated:ios": "NODE_ENV=production npx react-native bundle --platform ios --dev false --entry-file index.ts --bundle-output ios/main.jsbundle --config metro.config.obfuscation.js",
    "bundle:obfuscated:android": "NODE_ENV=production npx react-native bundle --platform android --dev false --entry-file index.ts --bundle-output android/app/src/main/assets/index.android.bundle --config metro.config.obfuscation.js",
    "verify:obfuscation": "node scripts/verify-obfuscation.js",
    "sourcemaps:extract": "node scripts/manage-sourcemaps.js extract",
    "sourcemaps:upload": "node scripts/manage-sourcemaps.js upload",
    "security:audit": "node scripts/security-audit.js",
    "validate:env": "node scripts/validate-env.js",
    "setup:secrets": "bash scripts/setup-eas-secrets.sh"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^7.0.18",
    "@react-navigation/stack": "^7.2.2",
    "@reduxjs/toolkit": "^2.6.1",
    "axios": "^1.6.7",
    "expo": "~51.0.0",
    "expo-av": "~14.0.7",
    "expo-blur": "~13.0.3",
    "expo-constants": "~16.0.2",
    "expo-haptics": "~13.0.1",
    "expo-linear-gradient": "~13.0.2",
    "expo-location": "~17.0.1",
    "expo-status-bar": "~1.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "^1.14.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-web": "^0.19.13",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.27.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.79",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ip": "^2.0.1",
    "javascript-obfuscator": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "metro-minify-terser": "^0.80.0",
    "prettier": "^3.2.5",
    "qrcode-terminal": "^0.12.0",
    "terser": "^5.27.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
