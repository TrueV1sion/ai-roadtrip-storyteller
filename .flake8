[flake8]
max-line-length = 120
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    .eggs,
    *.egg,
    build,
    dist,
    .pytest_cache,
    .mypy_cache,
    migrations,
    alembic,
    tests
per-file-ignores =
    __init__.py:F401
    backend/app/models.py:F401
    backend/app/schemas.py:F401